{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Aspire": {
    "Environment": {
      "Name": "Test",
      "IsLocal": false
    },
    "Resources": {
      "ApiService": {
        "Cpu": "0.25",
        "Memory": "0.5Gi",
        "Replicas": {
          "Min": 1,
          "Max": 2
        }
      },
      "WebFrontend": {
        "Cpu": "0.25",
        "Memory": "0.5Gi",
        "Replicas": {
          "Min": 1,
          "Max": 2
        }
      }
    },
    "Database": {
      "Provider": "Sqlite",
      "ConnectionString": "Data Source=fencemark-test.db"
    }
  }
}
