{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Aspire": {
    "Environment": {
      "Name": "Production",
      "IsLocal": false
    },
    "Resources": {
      "ApiService": {
        "Cpu": "1",
        "Memory": "2Gi",
        "Replicas": {
          "Min": 2,
          "Max": 10
        }
      },
      "WebFrontend": {
        "Cpu": "1",
        "Memory": "2Gi",
        "Replicas": {
          "Min": 2,
          "Max": 10
        }
      }
    },
    "Database": {
      "Provider": "SqlServer",
      "ConnectionString": ""
    }
  }
}
