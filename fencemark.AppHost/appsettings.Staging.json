{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Information"
    }
  },
  "Aspire": {
    "Environment": {
      "Name": "Staging",
      "IsLocal": false
    },
    "Resources": {
      "ApiService": {
        "Cpu": "0.5",
        "Memory": "1Gi",
        "Replicas": {
          "Min": 1,
          "Max": 3
        }
      },
      "WebFrontend": {
        "Cpu": "0.5",
        "Memory": "1Gi",
        "Replicas": {
          "Min": 1,
          "Max": 3
        }
      }
    },
    "Database": {
      "Provider": "SqlServer",
      "ConnectionString": ""
    }
  }
}
